{
  "name": "g0v-summit-2020",
  "version": "1.0.0",
  "description": "Official website for g0v Summit 2020",
  "contributors": [
    {
      "name": "mangoSu"
    },
    {
      "name": "ddio",
      "email": "ddio@ddio.io"
    }
  ],
  "private": true,
  "scripts": {
    "sync:article": "node tools/syncArticles.js",
    "sync:table": "node tools/syncStructuredData.js",
    "sync:proposal": "node tools/syncProposals.js",
    "sync:all": "npm run sync:article && npm run sync:table && npm run sync:proposal",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "generate:staging": "env DOMAIN=g0v.github.io ROUTER_BASE=/summit2020 nuxt generate",
    "generate:production": "env DOMAIN=summit.g0v.tw ROUTER_BASE=/2020 nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^3.0.0-0",
    "front-matter": "^3.2.1",
    "leaflet": "^1.6.0",
    "lodash": "^4.17.20",
    "markdown-it": "^11.0.0",
    "moment": "^2.27.0",
    "move-file": "^2.0.0",
    "nuxt": "^2.14.0",
    "nuxt-i18n": "^6.11.1",
    "nuxt-leaflet": "0.0.21",
    "vue-markdown": "^2.2.4",
    "vue2-leaflet": "^2.5.2",
    "yaml-front-matter": "^4.1.0"
  },
  "devDependencies": {
    "@hackmd/hackmd-cli": "^1.1.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "airtable": "^0.8.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "jest": "^24.1.0",
    "node-sass": "^4.14.1",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "raw-loader": "^4.0.1",
    "sass-loader": "^8.0.2",
    "tachyons": "^4.12.0",
    "tempfile": "^3.0.0",
    "vue-jest": "^4.0.0-beta.5",
    "yaml": "^1.10.0"
  }
}
